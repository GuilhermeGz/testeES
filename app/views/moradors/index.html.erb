<p id="notice"><%= notice %></p>

<h1>Moradors</h1>

<table>
  <thead>
    <tr>
      <th>Nome</th>
      <th>Cpf</th>
      <th>Bloco</th>
      <th>Casanum</th>
      <th>Contato</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @moradors.each do |morador| %>
      <tr>
        <td><%= morador.nome %></td>
        <td><%= morador.cpf %></td>
        <td><%= morador.bloco %></td>
        <td><%= morador.casanum %></td>
        <td><%= morador.contato %></td>
        <td><%= link_to 'Show', morador %></td>
        <td><%= link_to 'Edit', edit_morador_path(morador) %></td>
        <td><%= link_to 'Destroy', morador, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Morador', new_morador_path %>
